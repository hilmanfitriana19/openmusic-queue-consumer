{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "camelcase": "off",
    "no-underscore-dangle": "off",
    "object-curly-newline": "off",
    "comma-dangle": "off",
    "import/no-import-module-exports": "off",
    "quotes": ["error", "single"],
    "object-shorthand": "off",
    "operator-linebreak": "off"
  }
}
